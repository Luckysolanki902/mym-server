{
    "version": 2,
    "builds": [{ "src": "server.js", "use": "@vercel/node" }],
    "routes": [
      { "handle": "filesystem" },
      {
        "src": "/(.*)",
        "dest": "/server.js"
      }
    ],
    "env": {
      "NPM_USE_YARN": "true"
    },
    "functions": {
      "api": {
        "runtime": "nodejs14.x",
        "memory": 1024,
        "includeFiles": ["server.js", "package.json", "yarn.lock"]
      }
    }
  }
  